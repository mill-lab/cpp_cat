#include <iostream>
#include <cstring>
using namespace std;

int main()
{
  char str1[32], str2[32];
  int cmp;

  cout << "input str1 --";
  cin.getline(str1, 32);
  cout << "input str2 --";
  cin.getline(str2, 32);

  cmp = strcmp(str1, str2);
  if (cmp<0)
    cout << str1 << "は、" << str2 << "より前にあります" << endl;
  else if (cmp < 0)
    cout << str1 << "は、" << str2 << "より後ろにあります" << endl;
  else
    cout << str1 << "と" << str2 << "は、同じ文字列です" << endl;
}
  
  return 0;
}
