#include <iostream>
using namespace std;

int main()
{
  short a = 12, b = 100, c = -50;
  
  cout << (a << 1) << endl;
  cout << (a << 2) << endl;
  cout << (a << 3) << endl;
  cout << (a << 4) << endl;
  
  cout << (b >> 1) << endl;
  cout << (b >> 2) << endl;
  cout << (b >> 3) << endl;
  cout << (b >> 4) << endl;
  
  cout << (c << 1) << endl;
  cout << (c >> 1) << endl;
  
  return 0;
}
